{
  "name": "surge-client",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "nodemon ./bin/www",
    "watch": "watchify ./public/js/libs/surge-client.js --s Surge -o ./public/dist/js/surge-client.js",
    "build" : "browserify ./public/js/libs/surge-client.js --s Surge | uglifyjs -mc > ./public/dist/js/surge-client.min.js"
  },
  "dependencies": {
    "body-parser": "~1.12.4",
    "connect-mongo": "*",
    "cookie-parser": "~1.3.5",
    "cors": "^2.7.1",
    "debug": "~2.2.0",
    "ejs": "~2.3.1",
    "express": "~4.12.4",
    "express-session": "*",
    "mongoose": "*",
    "morgan": "~1.5.3",
    "serve-favicon": "~2.2.1",
    "sockjs-client": "^1.0.0"
  },
  "devDependencies": {
    "browserify": "latest",
    "nodemon": "^1.3.7",
    "uglify-js": "^2.4.23",
    "watchify": "latest"
  },
  "browser": {
    "surge": "./public/js/libs/surge-client.min.js"
  }
}
